{
  "hosting": {
    "public": "release", 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "src/**",
      "package*.json"
    ],
    "headers": [
      {
        "source":
"**/*.@(jpg|jpeg|gif|png|svg|webp|js|css|ttf|woff|woff2)",
"headers": [
{
  "key": "Cache-Control",
  "value": "public, max-age=604800, must-revalidate"
}]
      }
    ],
"rewrites": [
  {
    "source": "/ar/**",
    "destination": "/ar/index.html"
  },
  {
    "source": "**",
    "destination": "/en/index.html"
  }
]
  }
}